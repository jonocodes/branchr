{
  "public": {
    "serviceName": "Demo App",
    "localGitDir": "$SETTINGS_GIT",
    "repoLocallity": "local",
    "dockerBuildDir" : "$SETTINGS_GIT",
    "dockerComposeFile": "/tmp/app-compose.yml",
    "dockerBuildCmd": "docker build -t \$IMAGE .",
    "host": "$SETTINGS_HOST",

    "requiredPorts" : {
      "web" : [
        "PORT_HTTP_WEB"
      ],
      "redis" : [
        "PORT_TCP_REDIS"
      ]
    }
  }
}
